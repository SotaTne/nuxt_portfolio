{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "plugin:vue/vue3-essential",
    "standard-with-typescript",
    "prettier"
  ],
  "overrides": [
    {
      "files": ["pages/*.vue", "error.vue", "layouts/*.vue", "component/*.vue"],
      "rules": {
        "vue/multi-word-component-names": "off"
      }
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {}
}
